container_commands:
  00_makemigrations:
    command: "source /opt/python/run/venv/bin/activate && python weatherapp/manage.py makemigrations --noinput"
    leader_only: true
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python weatherapp/manage.py migrate --noinput"
    leader_only: true